{
  "name": "Google Translate Copy Paste",
  "description": "Google Translate Copy Paste",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://translate.google.com/*"
      ],
      "include_globs": [
        "*://translate.google.*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
